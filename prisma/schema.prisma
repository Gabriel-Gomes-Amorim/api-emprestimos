
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email String  @unique
  name  String
  cpf  String   @unique
  password String
  age Int
  location String
  income Decimal @db.Decimal(10, 2)

  @@map("user")

}